{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "@hint/utils": "^1.0.0",
    "@hint/utils-debugging-protocol-common": "^3.0.1",
    "chrome-launcher": "^0.10.4",
    "is-ci": "^2.0.0"
  },
  "description": "hint connector for Google Chrome",
  "devDependencies": {
    "@hint/utils-create-server": "^3.0.0",
    "@types/is-ci": "^1.1.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "11.12.0",
    "@types/sinon": "^7.0.11",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "ava": "^1.4.1",
    "cpx": "^1.5.0",
    "eslint": "^5.15.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-markdown": "^1.0.0",
    "hint": "^4.5.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist/src"
  ],
  "homepage": "https://webhint.io/",
  "keywords": [
    "connector",
    "chrome",
    "google chrome",
    "hint",
    "hint-connector-chrome",
    "webhint"
  ],
  "license": "Apache-2.0",
  "main": "./dist/src/connector.js",
  "name": "@hint/connector-chrome",
  "nyc": {
    "branches": 30,
    "extends": "../../.nycrc",
    "functions": 70,
    "include": [
      "dist/src/**/*.js"
    ],
    "lines": 70,
    "statements": 70
  },
  "peerDependencies": {
    "hint": "^4.5.0"
  },
  "repository": "webhintio/hint",
  "scripts": {
    "build": "npm-run-all build:*",
    "build-release": "npm run clean && npm run build:assets && tsc --inlineSourceMap false --removeComments true",
    "build:assets": "cpx \"./{src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc -b",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --cache --ext js --ext md --ext ts --ignore-path ../../.eslintignore --report-unused-disable-directives",
    "lint:dependencies": "node ../../scripts/lint-dependencies.js",
    "lint:md": "node ../../scripts/lint-markdown.js",
    "test": "npm run lint && npm run build && npm run test-only",
    "test-only": "nyc ava",
    "test-release": "npm run lint && npm run clean && npm run build:assets && tsc && npm run test-only",
    "watch": "npm run build && npm-run-all --parallel -c watch:*",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "version": "3.0.1"
}
