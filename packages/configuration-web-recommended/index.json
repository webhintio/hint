{
    "connector": {
        "name": "puppeteer"
    },
    "extends": [
        "accessibility"
    ],
    "formatters": [
        "html",
        "summary"
    ],
    "hints": [
        "button-type",
        "compat-api/css",
        "compat-api/html",
        "content-type",
        "create-element-svg",
        "css-prefix-order",
        "detect-css-reflows/composite",
        "detect-css-reflows/layout",
        "detect-css-reflows/paint",
        "disown-opener",
        "highest-available-document-mode",
        "html-checker",
        "http-cache",
        "http-compression",
        "leading-dot-classlist",
        "meta-charset-utf-8",
        "meta-viewport",
        "no-bom",
        "no-disallowed-headers",
        "no-friendly-error-pages",
        "no-html-only-headers",
        "no-http-redirects",
        "no-inline-styles",
        "no-protocol-relative-urls",
        "no-vulnerable-javascript-libraries",
        "scoped-svg-styles",
        "sri",
        "ssllabs",
        "strict-transport-security",
        "stylesheet-limits",
        "validate-set-cookie-header",
        "x-content-type-options"
    ],
    "hintsTimeout": 120000,
    "parsers": [
        "css",
        "javascript"
    ]
}
